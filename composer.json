{
  "name": "membrane/membrane",
  "type": "library",
  "license": "MPL-2.0",
  "autoload": {
    "psr-4": {
      "Membrane\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Membrane\\Tests\\": "tests/",
      "Membrane\\Tests\\Fixtures\\": "tests/fixtures/"
    }
  },
  "require": {
    "php": "^8.1.0",
    "cebe/php-openapi": "dev-all-membrane-3.1-fixes@dev",
    "psr/http-message": "^1.0 || ^2.0",
    "psr/log": "^2.0 || ^3.0",
    "symfony/console": "^6.2 || ^7.0",
    "membrane/openapi-reader": "dev-router-mvp-3.1-support@dev",
    "membrane/openapi-router": "dev-add-3.1-support@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5.38",
    "phpstan/phpstan": "^1.12.5",
    "squizlabs/php_codesniffer": "^3.7",
    "guzzlehttp/psr7": "^2.4",
    "mikey179/vfsstream": "^1.6.7"
  },
  "bin": [
    "bin/membrane"
  ],
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/charjr/php-openapi"
    }
  ],
  "minimum-stability": "dev"
}
